<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.cgi.mates</groupId>
	<artifactId>mates-web-client</artifactId>
	<version>1.0</version>
	<name>MATES Web Client</name>
	<url>http://maven.apache.org</url>
	<properties>
		<camel.version>2.13.0</camel.version>
	</properties>
	<dependencies>
		<dependency>
			<groupId>com.cgi.mates</groupId>
			<artifactId>mates-grammars</artifactId>
			<version>1.0</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>exec-maven-plugin</artifactId>
				<version>1.3</version>
				<executions>
					<execution>
						<phase>install</phase>
						<goals>
							<goal>exec</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<executable>make</executable>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
